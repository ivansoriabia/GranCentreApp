<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.ivan.grancentreapp">

    <application
        android:allowBackup="true"
        android:icon="@drawable/logo"
        android:label="@string/app_name"
        android:roundIcon="@drawable/logo"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".GranCentre"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".restaurants_index"
            android:screenOrientation="portrait" />
        <activity android:name=".restaurants_mediterrani" />
        <activity android:name=".restaurants_italia" />
        <activity android:name=".restaurants_internacional" />
        <activity android:name=".temps"></activity>
    </application>

</manifest>